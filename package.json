{
  "name": "dasm",
  "version": "4.1.0",
  "description": "An emscripten-compiled version of the dasm macro assembler",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "mocha test/.setup.js test/**/*-test-*.js",
    "test:watch": "mocha -w test/.setup.js test/**/*-test-*.js",
