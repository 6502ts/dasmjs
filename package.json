  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "mocha": "^3.2.0",
    "tslint": "^4.3.1",
    "typescript": "^2.4.1"
  }
