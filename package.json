{
  "name": "dasm",
  "version": "6.0.1",
  "description": "An emscripten-compiled version of the dasm macro assembler",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "jest --testMatch **/test/**/*-test-*.js",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watchAll",
    "build": "tsc --pretty",
    "watch": "tsc --pretty --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeh/dasmjs.git"
  },
  "keywords": [
    "dasm",
    "assembler",
    "assembly",
    "emscripten",
    "atari",
    "6502",
    "vcs",
    "2600",
    "6507"
  ],
  "author": "Zeh Fernando",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/zeh/dasmjs/issues"
  },
  "homepage": "https://github.com/zeh/dasmjs#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "core-js": "^2.4.1",
    "jest": "^23.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "path": "^0.12.7"
  }
}
